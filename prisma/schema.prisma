
generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
}

model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String
  role UserRole @default(USER)
  @@map("users")
}

enum UserRole {
  USER
  ADMIN
}